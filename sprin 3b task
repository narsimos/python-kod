def yas_kateqoriya():
    yas = int(input("Yasinizi daxil edin: "))
    if yas < 18:
        print("Genc")
    else:
        print("Yetkin")
10)
def soz_uzunluq():
    soz = input("Bir soz daxil edin: ")
    print(f"Soz uzunlugu: {len(soz)}")

