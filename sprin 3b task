5)
def toplam(*args):
    return sum(args)
6)
def ortalama(*args):
    if not args:
        return "Reqem yoxdur"
    return sum(args) / len(args)
7)
def adlar_reqemler(**kwargs):
    for ad, reqem in kwargs.items():
        print(f"ad: {ad}, reqem: {reqem}")
8)def tip_yoxla(deyer):
    if isinstance(deyer, str):
        print("metn")
    elif isinstance(deyer, (int, float)):
        print("reqem")
    else:
        print("basqa")
